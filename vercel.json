{
  "version": 2,
  "name": "rss-reader-bravo",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "RSS_SOURCES": "@rss_sources"
  },
  "functions": {
    "app/api/rss/route.ts": {
      "maxDuration": 30
    }
  }
}
